   ],
   "source": [
    "ps = PorterStemmer()\n",
    "sent = input(\"enter sentence: \")\n",
    "words = word_tokenize(sent)\n",
    "for w in words:\n",
    "    print(w, \":\", ps.stem(w))"
   ]
